{
  "version": 2,
  "builds": [{
    "src": "index.js",
    "use": "@vercel/node"
  }],
  "routes": [
    {
      "src": "/api",
      "dest": "/index.js",
      "methods": ["POST"],  
      "headers": {
        "Content-Type": "application/json"  
    },
    {
      "src": "/",
      "dest": "/index.js",
      "methods": ["GET"]  
    }
  ]
}
